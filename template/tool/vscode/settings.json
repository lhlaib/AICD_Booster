{
  "update.mode": "none",
  "extensions.autoUpdate": false,
  "extensions.autoCheckUpdates": false,
  "telemetry.telemetryLevel": "off",
  "files.associations": {
    "*.v": "verilog",
    "*.vh": "verilog",
    "*.sv": "systemverilog",
    "*.svh": "systemverilog",
    "*.bsv": "bluespec",
    "*.sp": "spice",
    "*.spice": "spice",
    "*.cir": "spice",
    "*.cdl": "spice",
    "*.sdc": "tcl",
    "*.xdc": "tcl",
    "*.tcl": "tcl",
    "*.sby": "ini",
    "*.rua": "plaintext",
    "*.def": "plaintext",
    "*.lef": "plaintext",
    "*.lib": "plaintext",
    "*.spef": "plaintext",
    "*.sdf": "plaintext",
    "*.cmd": "shellscript",
    "*.sh": "shellscript",
    "*.csh": "shellscript",
    "*.ksh": "shellscript",
    "*.log": "log"
  },
  "editor.tabSize": 2,
  "editor.insertSpaces": true,
  "editor.rulers": [80, 120],
  "editor.renderWhitespace": "boundary",
  "files.trimTrailingWhitespace": true,
  "files.insertFinalNewline": true,
  "workbench.startupEditor": "none"
}
